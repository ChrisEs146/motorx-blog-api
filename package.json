  "main": "./src/index.ts",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "env-cmd -f ./.env nodemon ./src/index.ts",
    "build": "tsc -p ./tsconfig.json",
    "lint": "eslint . --ext .ts",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "test:unit": "jest --testPathPattern=./test/unit --runInBand",
    "test:integration": "test --testPathPattern=./test/integration --runInBand"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.4.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@prisma/client": "^4.10.1",
